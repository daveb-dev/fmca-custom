name: jupyter_dashboards
channels:
  - conda-forge
  - nodefaults

dependencies:
  - cmake                # and any other build deps
  - git 
  - cxx-compiler
  - python >=3.9,<3.10    # or whatever version
  - jupyterlab >=3.4.4,<4 # or whatever version
  - pip
  - pip:
    - any-deps-not-met-by-conda
    - matplotlib 
  - jupyter_dashboards
  - jupyter_dashboards_bundlers
  
